{{! basic auth homepage implementation }}
{{#if user}}
    <div class='container'>

        <h1>Hello, {{user.fname}}!</h1>
        <p>This is your homepage!</p>
        <p>Click <a href='./logout'>here</a> to log out.</p>

    </div>
{{/if}}

<h1>My stuff:</h1>
<ul>
    {{#each allTestData}}
        <li>{{this.stuff}}</li>
    {{/each}}
</ul>

<h1>A dragon:</h1>
<img src='/images/Dragonite.png' />